#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nhillard/Documents/Code/Personal/natbaca.github.io/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_rollup@4.54.0_typescript@5.9.3/node_modules/astro/node_modules:/Users/nhillard/Documents/Code/Personal/natbaca.github.io/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_rollup@4.54.0_typescript@5.9.3/node_modules:/Users/nhillard/Documents/Code/Personal/natbaca.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nhillard/Documents/Code/Personal/natbaca.github.io/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_rollup@4.54.0_typescript@5.9.3/node_modules/astro/node_modules:/Users/nhillard/Documents/Code/Personal/natbaca.github.io/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_rollup@4.54.0_typescript@5.9.3/node_modules:/Users/nhillard/Documents/Code/Personal/natbaca.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.16.6_@types+node@25.0.3_rollup@4.54.0_typescript@5.9.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.16.6_@types+node@25.0.3_rollup@4.54.0_typescript@5.9.3/node_modules/astro/astro.js" "$@"
fi
